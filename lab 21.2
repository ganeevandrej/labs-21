#include <iostream>
#include <math.h>

using namespace std;
int main()
{
    int k, m, n, i, j;
    cin >> m;
    cin >> n;
    cin >> k;
    int a[10][10];
    for (i = 0; i < m; i++) {
        for (j = 0; j < n; j++)
            cin >> a[i][j];
    }

   int sum = 0, p = 1;
    for (j = 0; j < n; j++) {
        sum = sum + a[k-1, j];
        p = p * a[k-1, j];
    }

    cout << sum;
    cout << p;
    
    return 0;
}
