#include <iostream>

using namespace std;

int main() {
    int n, j, m, k;
    cin >> n;
    cin >> m;

    int a[10][10];
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            cin >> a[i][j];
        }
    }

    for ( j = 0; j < n; j++) {
        k = 0;
        for (int i = 0; i < m; i++)
            if ((a[i, j] % 2) = 0)
            {
                 k++;
            }

        if (k = 0)
            break;
    }
    cout << j;

    return 0;
}
