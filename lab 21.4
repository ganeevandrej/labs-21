#include <iostream>

using namespace std;

int main() {
    int n, j,  m, k, sum;
    float arif;
    cin >> n;
    cin >> m;
   
    int a[10][10];
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            cin >> a[i][j];
        }
    }

    for (int j = 0; j < m; j++) {
        sum = 0;
        k = 0;
        for (int i = 0; i < n; i++) {
        sum = sum + a[i, j];
        arif = sum / n;
        }
        for (int i = 0; i < n; i++)
            if (a[i, j] > arif)
            k++;
        }
    
     cout  << j;
     cout  << k;
    
    return 0;
}
